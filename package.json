    "drizzle-orm": "^0.44.6",
    "graphql": "^16.8.1",
    "lucide-react": "^0.544.0",
    "next": "16.0.6",
    "next-themes": "^0.4.6",
    "openai": "^6.8.1",
    "payload": "3.58.0",
    "pixi.js": "^8.14.3",
